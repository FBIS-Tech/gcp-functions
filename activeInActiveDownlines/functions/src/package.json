{
    "name": "google-cloud-functions-typescript",
    "version": "0.0.4",
    "description": "This is a TypeScript based Cloud Functions template.",
    "main": "index.js",
    "config": {
        "function_name": "activeRetailersStatus",
        "region": "europe-west3",
        "gcp_project": "asterisk-ivr-293907",
        "runtime": "nodejs14"
    },
    "scripts": {
        "clean": "rimraf \"functions/*\"",
        "lint": "tslint --project tsconfig.json",
        "tsc": "tsc",
        "cp-package": "copyfiles package.json functions/src",
        "build": "run-s clean lint tsc cp-package",
        "test": "mocha \"functions/test/\"",
        "deploy": "gcloud functions deploy $npm_package_config_function_name --runtime $npm_package_config_runtime --region=$npm_package_config_region --trigger-http --project $npm_package_config_gcp_project"
    },
    "bin": {
        "create-project": "./cli.js"
    },
    "keywords": [
        "cloud",
        "functions",
        "typescript"
    ],
    "author": "\"@flatfisher <kyoheitsuda@flatfish.jp> (https://kyohei.flatfish.jp)",
    "license": "MIT",
    "devDependencies": {
        "@types/body-parser": "^1.19.0",
        "@types/chai": "4.2.19",
        "@types/chai-http": "4.2.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.12",
        "@types/mocha": "8.2.2",
        "@types/moment": "2.13.0",
        "@types/mysql": "^2.15.18",
        "@types/node": "^14.17.4",
        "chai": "4.3.4",
        "chai-http": "4.3.0",
        "copyfiles": "2.4.1",
        "express": "^4.17.1",
        "mocha": "8.4.0",
        "npm-run-all": "4.1.5",
        "rimraf": "3.0.2",
        "tslint": "6.1.3",
        "typescript": "4.3.4"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "declarative-js": "^3.8.15",
        "dotenv": "^10.0.0",
        "exceljs": "^4.2.1",
        "moment": "^2.29.1",
        "momentjs": "^2.0.0",
        "mysql2": "^2.2.5"
    }
}
